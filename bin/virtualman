#!/usr/bin/env ruby

####Required
require 'virtualman'
include Interractive

menu = ["clone","cook","ssh","add","delete","bootify","exit"]

choice = "0"

while choice != "exit"
	puts "----VirtualMan----"
	puts "Please select an action to do with your VM."
	choice = Menu.unic_run(menu)
	Interractive.send(choice)
end

puts "Thank you come again!"